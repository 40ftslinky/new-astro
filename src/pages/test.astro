.t { grid-area: t; } */


.project-card {
	position: relative;
	/* object-fit: cover; */
	/* object-position: center center;
	width: 100%; 
	height: 100%;
	aspect-ratio: 4 / 3; */
	overflow: hidden;
	line-height: 1;
} 

.project-card {
	/* width: var(--max-width);
	max-width: var(--max-width);
	max-height: var(--max-width); */
}
.project-card img {
	/* min-height: unset; */
	/* max-height: 50vw; */
	aspect-ratio: 4 / 3; 
	overflow: hidden;
}

/* .project-card, */
.project-card[class="a"],
.project-card[class="b"],
.project-card[class="e"],
.project-card[class="f"],
.project-card[class="g"],
.project-card[class="h"],
.project-card[class="i"],
.project-card[class="j"] {
	width: var(--max-width);
}
.project-card[class="c"],
.project-card[class="d"],
.project-card[class="k"] {
	width: calc(90% * (2 / var(--col_number)));
}


.project-card::before {
	position: absolute;
	z-index: 111;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	content: '';
	background: rgba(0,0,0,0.15);
	/* top: 1rem;
	left: 1rem;
	width: 2rem;
	height: 2rem;
	color: white;
	counter-increment: grid;
	content: counter(grid, lower-alpha);
	filter: invert(1); */
	transition: var(--transition);
}
.project-card:hover::before {
	background: rgba(0,0,0,0.25);
}



.project-card img {
	min-width: 100%;
	/* min-height: 100%; */
	max-height: calc(var(--max-width)*4/3);
	object-fit: cover;
	object-position: center center;
	transform: scale(1.01);
	transition: var(--transition);
	line-height: 0;
	vertical-align: middle;
}
.project-card:hover img {
	transform: scale(1.05);
}

.project-card video {
	max-height: calc(var(--max-width)*4/3);
	width: 100%;
	height: 100%;
	object-fit: inherit;
	vertical-align: middle;
}

@media screen and (max-width: 1080px) {
	:root {
		--col_number: 3;
		/* --max-width: calc(90vw / var(--col_number)); */
	}

	.project-gallery {
		display: flex !important;
		flex-wrap: wrap;
		flex-direction: row;
		justify-content: center;
		gap:1rem;
	}
	.project-card {
		width: var(--max-width);
		max-width: var(--max-width);
		max-height: var(--max-width);
	}
	.project-card img {
		/* min-height: unset; */
		max-height: 50vw;
