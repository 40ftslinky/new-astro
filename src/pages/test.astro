<style>
	
:root {
	--col_number: 4;
	--gap_number: calc(var(--col_number) - 1);
	--col_width: calc(100% - (var(--gap_number)*1rem));
	--max-width: calc(var(--col_width) / var(--col_number));

}
.project_section {}

.project_cont {
	margin:0;
	width: 100%;
	display:flex;
	flex-direction: column;
	justify-content: center;
	padding: 0;
	gap: 1rem;
}

.project-card-wrap {
	/*  */
	
}

.project-card-wrap:hover .project_text_cont {
	opacity: 1;
}

.project_text_cont {
	position: absolute;
	z-index: 1111;
	bottom: 0;
	left: 0;
	height: 100%;
	
	/* display: block; */
	display: flex;
	flex-direction: column;
	justify-content: flex-end;
    align-items: flex-start;
	padding: 1.5rem 1.5rem;
	opacity: 0;
	transition: var(--transition);
}
.project-card:hover .project_text_cont {
	opacity: 1;
}
@media screen and (max-width: 1080px) {
	.project_text_cont {
		opacity: 1;
	}
}
.project_text_cont.announcement {
	opacity: 1;
	top: 0;
	bottom: unset;
	justify-content: space-between;

}

h2.project_title,
.project_title {
	line-height: 1;
    transition: all .2s ease;
    margin: 0;
	padding: 0;
	color: #fff !important;
	-webkit-text-fill-color: unset;
	
	text-transform: uppercase;
    font-family: 'Inter Tight', sans-serif;
    font-weight: 400;
	font-size: 1.5rem;
    letter-spacing: .02em;
    text-align: left;
	transition: var(--transition);
	transform: translateY(0.5rem);
}
.announcement h2.project_title,
.announcement .project_title {
    transform: translateY(0rem);
}


@media screen and (max-width: 1080px) {
	h2.project_title,
	.project_title {
		font-size: 1.25rem;
	}
}
@media screen and (max-width: 800px) {
	h2.project_title,
	.project_title {
		font-size: 1.125rem;
	}	
}
.project-card:hover .project_title {
	transform: translateY(0);
}
h2.project_announcement,
.project_announcement {
	line-height: 1;
    transition: all .2s ease;
    margin: 0;
	padding: 0;
	color: #fff !important;
	-webkit-text-fill-color: unset;
	
	text-transform: uppercase;
    font-family: 'Inter Tight', sans-serif;
    font-weight: 500;
	font-size: 2.125vw;
	/* font-size: clamp(1.875rem, 0.2679rem + 2.381vw, 3.125rem);    text-align: left; */
	transition: var(--transition);
	/* transform: translateY(-0.5rem); */

}
@media screen and (max-width: 1080px) {
	h2.project_announcement,
	.project_announcement {
		font-size: 3.125vw;
	}
}
@media screen and (max-width: 800px) {
	h2.project_announcement,
	.project_announcement {
		font-size: 4.25vw;
	}	
}
.project-card:hover .project_announcement {
	/* transform: translateY(0); */
}


/* Parent container of .project-card */
.project-gallery {
	display: grid;
	grid-template-columns: repeat(4, 1fr);
	grid-auto-flow: row;
	grid-auto-flow: dense;
	column-gap: 1rem;
	gap: 1rem 1rem;
	/* margin: 1rem; */
	width: 100%;
	background: rgba(145,145,145,0.125);
	grid-template-areas:
		"a b c c"
		"d d c c"
		"d d e f"
		/* "f f i j"
		"k l m m"
		"n o m m"
		"p p q r"
		"p p s t" */
		;
	
	counter-reset: grid;
}
.five-items {
	grid-template-areas:
		"g h k k"
		"i j k k"
		/* "n o m m"
		"p p q r"
		"p p s t" */
		;
}

.a { grid-area: a; }
.b { grid-area: b; }
.c { grid-area: c; }
.d { grid-area: d; }
.e { grid-area: e; }
.f { grid-area: f; }
.g { grid-area: g; }
.h { grid-area: h; }
.i { grid-area: i; }
.j { grid-area: j; }
.k { grid-area: k; }
/* .l { grid-area: l; }
.m { grid-area: m; }
.n { grid-area: n; }
.o { grid-area: o; }
.p { grid-area: p; }
.q { grid-area: q; }
.r { grid-area: r; }
